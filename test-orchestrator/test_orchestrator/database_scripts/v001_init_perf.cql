CREATE TABLE IF NOT EXISTS testtier.perf2(
    test_id uuid,
    host_id uuid,
    test_name text,
    date timestamp,
    cookie text,
    gateway_cpu_utilization float,
    gateway_cpu_free float,
    duration int,
    error_no int,
    error_text text,
    gateway_version text,
    host_size text,
    host_name text,
    ip text,
    ip_gateway text,
    local_port text,
    gateway_memory float,
    gateway_memory_free bigint,
    num_clients int,
    num_streams int,
    protocol text,
    raw_json text,
    rcvd double,
    rcvd_mbps double,
    remote_port text,
    sent int,
    sent_mbps double,
    test_type int,
    tier text,
    totalreceived double,
    totalsent double,
    version text,
    session_key text,
    PRIMARY KEY (host_id, test_id)
)  WITH CLUSTERING ORDER BY (test_id ASC);

