CREATE TABLE IF NOT EXISTS testtier.perf_results(
    test_id uuid,
    test_name text,
    result text,
    date timestamp,
    gateway_cpu_utilization float,
    gateway_cpu_free float,
    duration int,
    error_no int,
    error_text text,
    failed_rate int,
    gateway_version text,
    client_version text,
    host_size text,
    ip_gateway text,
    gateway_memory float,
    gateway_memory_free bigint,
    num_clients int,
    avg_mbps double,
    max_mbps double,
    min_mbps double,
    tier text,
    protocol text,
    packetsPerSec int,
    test_mode text,
    PRIMARY KEY (test_id)
);